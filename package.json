{
  "name": "MapJS",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build-dev": "tsc && vite build",
    "check": "npm run typings && npm run lint",
    "typings": "tsc",
    "build": "npm run build:types && npm run build:js",
    "build:js": "rollup  --config rollup.config.ts --configPlugin @rollup/plugin-typescript --no-stdin",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint ./src --ext ts,tsx src --color --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.12.10",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "rollup": "^4.21.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-alias": "^5.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "tslib": "^2.7.0"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "clsx": "^2.1.1",
    "antd": "5.17.0",
    "ol": "^10.1.0",
    "dayjs": "^1.11.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.25.0",
    "lodash": "^4.17.21",
    "@tanstack/react-query": "^5.51.9",
    "@tanstack/react-query-devtools": "^5.51.9",
    "react-i18next": "^14.0.0",
    "i18next": "^23.0.0",
    "i18next-http-backend": "^2.0.0",
    "zustand": "^4.5.4"
  }
}
